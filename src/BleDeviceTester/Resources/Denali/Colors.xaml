<?xml version="1.0" encoding="utf-8" ?>

<ResourceDictionary
    x:Class="RvLinkDeviceTester.Resources.Denali.Colors"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    >

    <Color x:Key="PageBackgroundColor">White</Color>
    <Color x:Key="ColorOnBackground">Black</Color>
    <!--  Set Dark Page color to White because we don't support Dark Mode yet.  -->
    <Color x:Key="DarkPageBackgroundColor">White</Color>
    <Color x:Key="ColorOnPrimary">White</Color>
    <Color x:Key="TextColorOnPrimary">White</Color>
    <Color x:Key="SecondaryTextColor">#7A7A7A</Color>
    <Color x:Key="MinimumContrastGray">#767676</Color>

</ResourceDictionary>